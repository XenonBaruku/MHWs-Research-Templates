        QWORD _UnknownQWORD;
        QWORD _UnknownQWORD;
        uint64 SimpleConstraintTableOffset;
        uint64 SimpleConstraintSourceTableOffset;
        QWORD _UnknownQWORD;
        uint64 SectionTypeOffset;
        local uint64 returnPos = FTell();
