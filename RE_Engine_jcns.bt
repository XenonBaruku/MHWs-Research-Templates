        uint16 ExtraJointAmount;
        uint16 ConstraintAmount;
        uint16 ReferenceAmount;
        uint16 _UnknownUINT16;
        QWORD _UnknownQWORD;
        QWORD _UnknownQWORD;
        break;
    case 16: // RE8
